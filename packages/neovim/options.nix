{
  encoding = "utf-8";
  fileencoding = "utf-8";
  fileformats = "unix,dos,mac";
  showtabline = 2;
  shiftwidth = 4;
  softtabstop = 4;
  tabstop = 8;
  expandtab = true;
  smarttab = true;
  autoindent = true;
  smartindent = true;
  breakindent = true;
  mouse = "a";
  visualbell = true;
  emoji = true;
  backup = false;
  ignorecase = true;
  smartcase = true;
  ambiwidth = "double";
  shell = "fish";
  swapfile = false;
  termguicolors = true;
  pumblend = 25;
  number = true;
  list = true;
  cursorline = true;
  listchars = {
    space = "･";
    tab = ">-";
    eol = "¬";
    extends = "»";
    precedes = "«";
    nbsp = "+";
    trail = "~";
  };
  laststatus = 3;
  signcolumn = "yes";
  colorcolumn = [
    80
    100
    120
  ];
  showmatch = true;
  scrolloff = 5;
  relativenumber = true;
}
